  pod 'Firebase/Messaging', '~> 5.3.0'
  
  #RNFirebase Firestore Package
  pod 'Firebase/Firestore', '~> 5.3.0'


  target 'anchorTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
